    "jest-puppeteer": "^4.3.0",
    "mini-css-extract-plugin": "^0.8.0",
    "puppeteer": "^1.19.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "core-js": "^3.0.1",
    "dragula-with-animation": "^3.7.4",
    "ramda": "^0.26.1",
    "rxjs": "^6.5.2"
